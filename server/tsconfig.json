{
  "compilerOptions": {
    "incremental": true,
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "ESNext"
    ],
    "allowJs": false,
    "declaration": false,
    "outDir": "../bin",
    "rootDir": "./",
    "tsBuildInfoFile": "../.cache/ts/server",
    "importHelpers": true,
    // "isolatedModules": true,              
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}